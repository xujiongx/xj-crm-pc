{
  "compilerOptions": {
    "module": "esnext",
    "target": "ES5",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "declaration": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "exclude": [
    "**/es",
    "**/public",
    "**/assets",
    "**/node_modules",
    "**/dist",
    "**/.umi",
    "**/.umi-production",
    "node_modules",
    "aicc-web"
  ]
}
