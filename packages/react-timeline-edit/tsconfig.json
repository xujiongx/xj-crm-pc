{
  "compilerOptions": {
    "module": "esnext",
    "target": "ES5",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "allowJs": true,
    // "importHelpers": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "baseUrl": "./",
    "strict": false,
    "declaration": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["src/.umi/*"],
    },
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "**/es",
    "**/public",
    "**/assets",
    "**/node_modules",
    "**/dist",
    "**/.umi",
    "**/.umi-production",
    "node_modules",
    "aicc-web"
  ]
}
